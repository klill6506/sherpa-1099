						<Description>Unemployment compensation</Description>
						<LineNumber>Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="StateLclRefundCreditOffsetAmt" type="USDecimalAmountNNType" minOccurs="0">
