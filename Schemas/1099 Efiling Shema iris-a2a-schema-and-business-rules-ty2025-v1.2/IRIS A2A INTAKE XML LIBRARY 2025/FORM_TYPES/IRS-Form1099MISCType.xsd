			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Form 1099 MISC Detail Type</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2023-01-10</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Initial version</VersionDescriptionTxt>
					<DescriptionTxt>The content model for the form 1099 misc.</DescriptionTxt>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
