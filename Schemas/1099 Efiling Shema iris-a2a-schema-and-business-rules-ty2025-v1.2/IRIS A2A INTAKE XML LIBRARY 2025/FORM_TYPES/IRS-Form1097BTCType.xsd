				<xsd:annotation>
					<xsd:documentation>
						<Description>Receiving Entity or person distributing part or all of the credit</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TotalCreditsAmt" type="USDecimalAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total amount of credits distributed to recipient for the calendar year</Description>
						<LineNumber>Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IssuerAssignedUniqueIdCd" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Unique identification code</Description>
						<LineNumber>Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="StringType">
						<xsd:enumeration value="A"/>
						<xsd:enumeration value="C"/>
						<xsd:enumeration value="O"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="IssuerAssignedUniqueId" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Unique identifier assigned by form issuer</Description>
						<LineNumber>Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="TextType">
						<xsd:maxLength value="39"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="BondTypeCd">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Bond Type</Description>
						<LineNumber>Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="StringType">
						<xsd:enumeration value="101"/>
						<xsd:enumeration value="199"/>
