				<xsd:annotation>
					<xsd:documentation>
						<Description>Recipient Detail</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecipientAccountNum" type="Text30Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Recipient Account Number</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PSEInd" type="DigitBooleanType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Check to indicate if FILER is a Payment settlement entity (PSE)</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EPFOrOtherThirdPartyInd" type="DigitBooleanType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Check to indicate if FILER is an Electronic Payment Facilitator (EPF)/Other third party</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PaymentCardInd" type="DigitBooleanType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Check to indicate transactions reported are Payment card</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ThirdPartyNetworkInd" type="DigitBooleanType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Check to indicate transactions reported are Third party network</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PSENm" type="PersonNameType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>PSE's name, if the entity with the filing requirement (payer) is an EPF or other third party</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PSEPhoneNum" type="ContactPhoneNumberType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>PSE's telephone number, if the entity with the filing requirement (payer) is an EPF or other third party</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SecondTINNoticeInd" type="DigitBooleanType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Indicator that payer was notified by the IRS twice within 3 calendar years that the payee provided an incorrect TIN.</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="GrossAmt" type="USDecimalAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Gross amount of payment card/third party network transactions</Description>
						<LineNumber>Line 1a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CardNotPresentTransAmt" type="USDecimalAmountNNType" minOccurs="0">
