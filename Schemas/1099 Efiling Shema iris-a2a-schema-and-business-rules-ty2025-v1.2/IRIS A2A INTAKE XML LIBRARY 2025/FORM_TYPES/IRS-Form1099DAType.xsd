	<xsd:complexType name="Form1099DATotalByStateGrpType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Form 1099 DA Total by State Group Type</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
