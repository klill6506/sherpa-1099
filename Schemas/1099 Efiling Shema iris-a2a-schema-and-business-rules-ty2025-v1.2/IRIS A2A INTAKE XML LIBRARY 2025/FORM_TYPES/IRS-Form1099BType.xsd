				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProceedsAmt" type="USDecimalAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>The amount of proceeds</Description>
						<LineNumber>Line 1d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CostOrOtherBasisAmt" type="USDecimalAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>The amount of property cost or other basis</Description>
						<LineNumber>Line 1e</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AccruedMarketDiscountAmt" type="USDecimalAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Accrued market discount interest the taxpayer receives</Description>
						<LineNumber>Line 1f</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NondeductibleWashSaleLossAmt" type="USDecimalAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>The amount of nondeductible loss in a wash sale transaction.</Description>
						<LineNumber>Line 1g</LineNumber>
