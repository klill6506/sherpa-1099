				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MailingAddressGrp" type="AddressGrpType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Mailing Address Group</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Timestamp Type - Timezone portion is required and fractional seconds are prohibited -->
	<xsd:simpleType name="TimestampType">
		<xsd:annotation>
			<xsd:documentation>Base type for a date and time stamp</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:dateTime">
			<xsd:pattern value="[1-9][0-9]{3}\-([0][1-9]|[1][0-2])\-([0][1-9]|[1-2][0-9]|[3][0-1])[T](([0][0-9]|[1][0-9]|[2][0-3])([:][0-5][0-9]){2}([.][0-9]+)?|[2][4]([:][0][0]){2}([.][0]+)?)([Z]|[\+\-]([0][0-9]|[1][0-4])[:][0-5][0-9])?"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- DAUnitCountPosType -->
	<xsd:simpleType name="DAUnitQuantityPosType">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(([0-9]{1,32})(\.[0-9]{1,18})?)"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Text 40 Type -->
	<xsd:simpleType name="Text40Type">
		<xsd:restriction base="TextType">
			<xsd:maxLength value="40"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Two Digit Code Type -->
	<xsd:simpleType name="TwoDigitCodeType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>
