	<!-- Type for Alpha-Numeric -->
	<xsd:simpleType name="AlphaNumericType">
		<xsd:restriction base="StringType">
			<xsd:maxLength value="150"/>
			<xsd:pattern value="[A-Za-z0-9]*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Person First Name Type -->
	<xsd:simpleType name="PersonFirstNameType">
		<xsd:annotation>
			<xsd:documentation>Typically used for a person's first name. Legal Characters: A-Z, a-z, hyphen and single space. Illegal Character: leading space, trailing space, adjacent spaces, and other symbols.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="20"/>
			<xsd:pattern value="([A-Za-z\-] ?)*[A-Za-z\-]"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Person Last Name Type -->
	<xsd:simpleType name="PersonLastNameType">
		<xsd:annotation>
			<xsd:documentation>Typically used for a person's last name. Legal Characters: A-Z, a-z, hyphen and single space. Illegal Character: leading space, trailing space, adjacent spaces, and other symbols.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="20"/>
			<xsd:pattern value="([A-Za-z\-] ?)*[A-Za-z\-]"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Submission ID Type - 8 numeric characters excluding 0 -->
	<xsd:simpleType name="SubmissionIdType">
		<xsd:restriction base="StringType">
			<xsd:maxLength value="8"/>
			<xsd:pattern value="[1-9][0-9]*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CUSIPNumberType">
		<xsd:restriction base="AlphaNumericType">
			<xsd:maxLength value="9"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Code4Type">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Code 4 Type</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2007-03-16</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Initial version</VersionDescriptionTxt>
					<DescriptionTxt>Code value that is limited to a 4 character string</DescriptionTxt>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="TextType">
			<xsd:length value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Type for a U.S. Address -->
	<xsd:complexType name="USAddressType">
		<xsd:sequence>
			<xsd:element name="AddressLine1Txt" type="StreetAddressType">
				<xsd:annotation>
					<xsd:documentation>Address line 1</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AddressLine2Txt" type="StreetAddressType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Address line 2</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CityNm" type="CityType">
				<xsd:annotation>
					<xsd:documentation>City</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="StateAbbreviationCd" type="StateType">
				<xsd:annotation>
					<xsd:documentation>State</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ZIPCd" type="ZIPCodeType">
				<xsd:annotation>
					<xsd:documentation>ZIP code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Type for a Business Name -->
	<xsd:complexType name="BusinessNameType">
		<xsd:sequence>
			<xsd:element name="BusinessNameLine1Txt" type="BusinessNameLine1Type">
				<xsd:annotation>
					<xsd:documentation>Business name line 1</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BusinessNameLine2Txt" type="BusinessNameLine2Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Business name line 2</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Person Full Name Type -->
	<xsd:complexType name="PersonFullNameType">
		<xsd:sequence>
			<xsd:element name="PersonFirstNm" type="PersonFirstNameType">
				<xsd:annotation>
					<xsd:documentation>Person First Name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PersonLastNm" type="PersonLastNameType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Person Last Name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="ContactPhoneNumberType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Contact Phone Number Type</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2018-01-01</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Update length to 30</VersionDescriptionTxt>
					<DescriptionTxt>Phone number of the person to be contacted. Omit hyphens and
						include extensions if applicable.</DescriptionTxt>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="10"/>
			<xsd:maxLength value="30"/>
			<xsd:pattern value="([0-9,])*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DigitBooleanType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Digit Boolean Type</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionDescriptionTxt>Initial Version</VersionDescriptionTxt>
					<VersionEffectiveBeginDt>2015-08-26</VersionEffectiveBeginDt>
					<DescriptionTxt>Type for digit boolean values. 0= False, 1=True</DescriptionTxt>
					<Status>A</Status>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="0"/>
			<xsd:enumeration value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="EnterpriseBusCorrelationIdType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Enterprise Business Correlation Identifier Type</DictionaryEntryNm>
					<MajorVersionNum>3</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2022-01-12</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Replace with IRIS-only version</VersionDescriptionTxt>
					<DescriptionTxt>Unique Transmission ID. A business correlation identifier used for all request and response enterprise messages.</DescriptionTxt>
					<Status>A</Status>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:pattern value="[0-9a-zA-Z]{8}-[0-9a-zA-Z]{4}-[0-9a-zA-Z]{4}-[0-9a-zA-Z]{4}-[0-9a-zA-Z]{12}:IRIS:[a-zA-Z0-9]{5}::(A|U)"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Integer Type - Non-Negative -->
	<xsd:simpleType name="IntegerNNType">
		<xsd:annotation>
			<xsd:documentation>Base type for a non-negative integer</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:nonNegativeInteger">
			<xsd:totalDigits value="25"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Integer 15 Type - Non-Negative -->
	<xsd:simpleType name="Integer15NNType">
		<xsd:annotation>
			<xsd:documentation>Base type for a non-negative integer with up to 15 digits</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:nonNegativeInteger">
			<xsd:totalDigits value="15"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Integer Type - Positive -->
	<xsd:simpleType name="IntegerPosType">
		<xsd:annotation>
			<xsd:documentation>Base type for a positive integer</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:positiveInteger">
			<xsd:totalDigits value="25"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="JuratPersonTitleType">
		<xsd:annotation>
			<xsd:documentation>Typically used for a jurat person's title. Legal Characters: A-Z, a-z, hyphen and single space. Illegal Character: leading space, trailing space, adjacent spaces, and other symbols</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="35"/>
			<xsd:pattern value="([A-Za-z\-] ?)*[A-Za-z\-]"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="JuratSignaturePINType">
		<xsd:annotation>
			<xsd:documentation>Type for Jurat Signature PIN (Personal Identification Number)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{5}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Miles Non-Negative Type -->
	<xsd:simpleType name="MilesNNType">
		<xsd:restriction base="IntegerNNType">
			<xsd:totalDigits value="12"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NameTextType">
		<xsd:annotation>
			<xsd:documentation>Name Text Type Legal Characters: A-Z, a-z, 0-9, hash, hyphen, parentheses, ampersand, apostrophe and single space. Illegal Character: leading space, trailing space, adjacent spaces, and other symbols.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="75"/>
			<xsd:pattern value="(([A-Za-z0-9#\-\(\)]|&#x26;|&#x27;) ?)*([A-Za-z0-9#\-\(\)]|&#x26;|&#x27;)"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PersonMiddleNameType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Person Middle Name Type</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2015-09-23</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Initial Version</VersionDescriptionTxt>
					<DescriptionTxt>Typically used for a person's Middle name. Legal Characters: A-Z, a-z, hyphen and single space. Illegal Character: leading space, trailing space, adjacent spaces, and other symbols.</DescriptionTxt>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="20"/>
			<xsd:pattern value="([A-Za-z\-] ?)*[A-Za-z\-]"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Person Title Type -->
	<xsd:simpleType name="PersonTitleType">
		<xsd:annotation>
			<xsd:documentation>Typically used for a person's title. Legal Characters: printable characters from &#x21; to &#x7E; plus single space. Illegal Character: leading space, trailing space, adjacent spaces.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="35"/>
			<xsd:pattern value="([&#x0021;-&#x007E;] ?)*[&#x0021;-&#x007E;]"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RecordIdType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Unique Record Identification Type</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2014-11-03</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Initial Version</VersionDescriptionTxt>
					<DescriptionTxt>Unique Record identification type.</DescriptionTxt>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="StringType">
			<xsd:maxLength value="8"/>
			<xsd:pattern value="[1-9][0-9]*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SuffixNameType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Suffix Name Type</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2016-06-29</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Initial Version</VersionDescriptionTxt>
					<DescriptionTxt>Typically used for a person's Suffix. Legal Characters: A-Z, a-z, hyphen and single space. Illegal Character: leading space, trailing space, adjacent spaces, and other symbols.</DescriptionTxt>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="20"/>
			<xsd:pattern value="([A-Za-z\-] ?)*[A-Za-z\-]"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TCCType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Transmitter Control Code (TCC) Type</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2014-11-03</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Initial Version</VersionDescriptionTxt>
					<DescriptionTxt>Transmitter Control Code (TCC) Type</DescriptionTxt>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[a-zA-Z0-9]{5}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TestCdType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Test Code Type</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2014-02-05</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Initial version</VersionDescriptionTxt>
					<DescriptionTxt>Identifies if the request is production request or a test request.  The expected values are "P" (to indicate that this is a production request) or "T" (to indicate that this is a test request).</DescriptionTxt>
					<DataElementId>174478</DataElementId>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="T"/>
			<xsd:enumeration value="P"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TINType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>TIN Type</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2011-03-21</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Initial version</VersionDescriptionTxt>
					<DescriptionTxt>Type for Taxpayer Identification Number - restricted to 9
						digits</DescriptionTxt>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{9}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Social Security Number Type -->
	<xsd:simpleType name="SSNType">
		<xsd:annotation>
			<xsd:documentation>Type for Social Security No. - 9 digits</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{9}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Employer Identification Number Type -->
	<xsd:simpleType name="EINType">
		<xsd:annotation>
			<xsd:documentation>Type for Employer Identification No. - 9 digits</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{9}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TotalNumberNonNegativeType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Total Number Non Negative Type</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2015-01-02</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Initial Version</VersionDescriptionTxt>
					<DescriptionTxt>Total Number Non Negative Type used for Counts.</DescriptionTxt>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:nonNegativeInteger">
			<xsd:totalDigits value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TransmissionTypeCdType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Transmission Type Code</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2015-01-15</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Initial version</VersionDescriptionTxt>
					<DescriptionTxt>Transmission Type Indicates whether Transmission has Original, Replacement or Corrected Records, Duplicate Record</DescriptionTxt>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="O"/>
			<xsd:enumeration value="C"/>
			<xsd:enumeration value="R"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="UniqueSubmissionIdType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>UniqueSubmission Identifier</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2015-01-06</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Initial Version</VersionDescriptionTxt>
					<DescriptionTxt>The unique submission identifier is as follows: ReceiptId | SubmissionId</DescriptionTxt>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[1-9][0-9]{3}\-[0-9]{11}\-[0-9a-zA-Z]{9}\|[1-9][0-9]{0,7}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Type for VINs -->
	<xsd:simpleType name="VINType">
		<xsd:annotation>
			<xsd:documentation>Type for Vehicle Identification No. - 1 to 17 or 19 alphanumeric characters</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z0-9]{1,17}"/>
			<xsd:pattern value="[A-Z0-9]{19}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="JuratSignatureGrpType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Jurat Signature Group Type</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2015-09-01</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Initial version</VersionDescriptionTxt>
					<DescriptionTxt>A group that wraps jurat signature information.</DescriptionTxt>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SignatureIntentInd" type="DigitBooleanType">
				<xsd:annotation>
					<xsd:documentation>
						<Component>
							<DictionaryEntryNm>Signature Intent Indicator</DictionaryEntryNm>
						</Component>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="JuratSignaturePIN" type="JuratSignaturePINType">
				<xsd:annotation>
					<xsd:documentation>
						<Component>
							<DictionaryEntryNm>Jurat Signature PIN (Personal Identification Number)</DictionaryEntryNm>
						</Component>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SignatureDt" type="DateNoTimeZoneType">
				<xsd:annotation>
					<xsd:documentation>
						<Component>
							<DictionaryEntryNm>Signature Date</DictionaryEntryNm>
						</Component>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="JuratPersonTitleTxt" type="JuratPersonTitleType">
				<xsd:annotation>
					<xsd:documentation>
						<Component>
							<DictionaryEntryNm>Person Title Text</DictionaryEntryNm>
						</Component>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PersonNm" type="PersonNameType">
				<xsd:annotation>
					<xsd:documentation>
						<Component>
							<DictionaryEntryNm>Person Name</DictionaryEntryNm>
						</Component>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OtherCompletePersonNameType">
		<xsd:annotation>
			<xsd:documentation>
				<Description>Group that wraps other completed person name details where all elements are optional.</Description>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PersonFirstNm" type="PersonFirstNameType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Person First Name</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PersonMiddleNm" type="PersonMiddleNameType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Person Middle Name</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PersonLastNm" type="PersonLastNameType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Person Last Name</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SuffixNm" type="SuffixNameType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Suffix Name</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AddressGrpType">
		<xsd:annotation>
			<xsd:documentation>
				<DescriptionTxt>The mailing address group is a group that wraps a choice between USAddress/ForeignAddress</DescriptionTxt>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="USAddress" type="USAddressType">
				<xsd:annotation>
					<xsd:documentation>
						<DescriptionTxt>Type for a U.S. Address</DescriptionTxt>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ForeignAddress" type="ForeignAddressType">
				<xsd:annotation>
					<xsd:documentation>
						<DescriptionTxt>Type for a Foreign Address</DescriptionTxt>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:simpleType name="UniqueRecordIdType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Unique Record Identifier</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2015-01-06</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Initial Version</VersionDescriptionTxt>
					<DescriptionTxt>The Unique Record Identifier is as follows:
									ReceiptID | SubmissionId | RecordId</DescriptionTxt>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[1-9][0-9]{3}\-[0-9]{11}\-[0-9a-zA-Z]{9}\|[1-9][0-9]{0,7}\|[1-9][0-9]{0,7}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MediaSourceCdType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Media Source Code Type</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2021-06-03</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Initial version</VersionDescriptionTxt>
					<DescriptionTxt>Media Source Code Type</DescriptionTxt>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="StringType">
			<xsd:enumeration value="M"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SubmissionChannelCdType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Submission Channel Code Type</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2022-02-17</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Initial version</VersionDescriptionTxt>
					<DescriptionTxt>Submission Channel Code Type. Taxpayers should set the value to A2A, UI is IRS-only.</DescriptionTxt>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="StringType">
			<xsd:enumeration value="A2A"/>
			<xsd:enumeration value="UI"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="VendorCdType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Vendor Indicator Type</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2014-11-03</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>Initial Version</VersionDescriptionTxt>
					<DescriptionTxt>Enter the appropriate code, V or I, indicating if software was provided by a vendor or produced in-house.</DescriptionTxt>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="StringType">
			<xsd:enumeration value="V"/>
			<xsd:enumeration value="I"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="EmailAddressType">
		<xsd:annotation>
			<xsd:documentation>
				<Component>
					<DictionaryEntryNm>Email Address Type</DictionaryEntryNm>
					<MajorVersionNum>1</MajorVersionNum>
					<MinorVersionNum>0</MinorVersionNum>
					<VersionEffectiveBeginDt>2008-01-08</VersionEffectiveBeginDt>
					<VersionDescriptionTxt>EFileTypes, December 14, 2007</VersionDescriptionTxt>
					<DescriptionTxt>Used for an email address - up to 75 characters</DescriptionTxt>
				</Component>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="TextType">
			<xsd:maxLength value="75"/>
			<xsd:pattern value=".+@.+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TINSubmittedTypeCdType">
		<xsd:restriction base="StringType">
			<xsd:enumeration value="INDIVIDUAL_TIN">
				<xsd:annotation>
					<xsd:documentation>Individual TIN</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BUSINESS_TIN">
